<%- include("layout", { title, body: `
  <h1>Projects</h1>
  <div class="grid">
    ${projects.map(p => `
      <div class="tile">
        <h2>${p.title}</h2>
        <p>${p.description}</p>
        <p class="tags">${p.stack.map(s => `<span>${s}</span>`).join("")}</p>
        <a href="${p.link}" target="_blank" rel="noreferrer">View Repo ?</a>
      </div>
    `).join("")}
  </div>
`}) %>
